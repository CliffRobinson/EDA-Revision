  "main": "server/index",
  "scripts": {
    "dev": "run-p dev:server dev:client",
    "dev:client": "webpack --watch",
    "dev:server": "nodemon --watch server",
    "test": "jest --watchAll",
    "start": "node server"
  },
  "author": "EDA",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
